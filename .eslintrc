{
	"parser": "babel-eslint",
	"plugins": [
		"react",
        "jsx-a11y"
	],
	"parserOptions": {
        "modules": true,
        "spread" : true,
        "restParams" : true,
        "sourceType": "module"
    },
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "rules" : {
        "no-undef" : 2
    }
}